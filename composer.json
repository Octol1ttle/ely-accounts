    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "git@gitlab.ely.by:elyby/amqp-controller.git"
        },
        {
            "type": "git",
            "url": "git@gitlab.ely.by:elyby/email-renderer.git"
        },
        {
            "type": "git",
            "url": "git@github.com:erickskrauch/Codeception.git"
        },
        {
            "type": "git",
            "url": "git@github.com:erickskrauch/yii2-queue.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "api\\": "api",
            "common\\": "common",
            "console\\": "console"
        },
        "files": [
            "common/consts.php"
        ]
    }
