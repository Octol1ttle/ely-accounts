        "emarref/jwt": "~1.0.3",
        "goaop/framework": "^2.2.0",
        "guzzlehttp/guzzle": "^6.0.0",
        "league/oauth2-server": "^4.1",
        "mito/yii2-sentry": "^1.0",
        "paragonie/constant_time_encoding": "^2.0",
        "ramsey/uuid": "^3.5",
        "spomky-labs/otphp": "^9.0.2",
        "webmozart/assert": "^1.2.0",
        "yiisoft/yii2": "2.0.15.1",
