{
    "name": "elyby/accounts",
    "description": "Authentication service for the Ely.by",
    "homepage": "https://account.ely.by",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
